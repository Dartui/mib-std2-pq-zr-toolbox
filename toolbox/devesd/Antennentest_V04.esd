#######################
screen FM-Tracing Antennentests
#######################
BIOSCtrl
value int per 0x01c100e7 72
label "debug destination"
entry "off" 0
entry "SD1" 1
poll 5000

table
content per 0x02c80122 1
label "FM Tuner 1-2"
poll 100
maxrows 2

orientation horizontal

columns ( "FRQ" int 75 ) ( "PI" String 70 ) ( "PS" String 130 ) ( "Q %" int 60 1 "#;'       -                 '" ) ( "FS dBµV" int 100 1 "#;'       -          '" ) ( "RDQ" int 60 1 "#;'       -        '" ) ( "MP" int 40 1 "#;'       -        '" ) ( "AP" int 40 1 "#;'   -        '" ) ( "M" int 40 1 "#;'   -       '" ) 
 

