# store LogFiles to SD-Card

########################################
# Name        nav_nvm_store_enable
# Description Trigger NVM copy process
########################################
# letzter Parameter 0 oder 1 keine Wirkung
# task: migrate from 200 to 100200
button
value int per 0x01c500e6 200 1
label "Copy files to SD"
poll 1000

########################################
# Name        nav_nvm_store_sd1
# Description Target directory of files.
# type        string
########################################
# task: migrate from 201 to 100201
choice
value per 0x01c500e6 201
label "Copy to /media/mp000"
poll  1000

########################################
# Name        nav_nvm_store_sd2
# Description Target directory of files.
# type        string
########################################
# task: migrate from 202 to 100202
choice
value per 0x01c500e6 202
label "Copy to /media/mp001"
poll  1000

########################################
# Name        nav_nvm_store_state
# Description Copy progress detail message.
# unit        
# type        string
########################################
# task: migrate from 203 to 100203
keyvalue
value String per 0x01c500e6 203
label "Copy Progress Details:"
poll  1000

########################################
# Name        nav_nvm_store_progress
# Description Copy progress string.
# unit        
# type        string
########################################
# task: migrate from 204 to 100204
keyvalue
value String per 0x01c500e6 204
label "Copy Progress:"
poll  1000

