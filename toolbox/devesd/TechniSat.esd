#######################
screen Power_TSD TechniSat
#######################
choice
value per 0x01f300fa 100000
label "TSD Enable Watchdog Reset"
poll 500


#######################
screen SystemCheck TechniSat
#######################
button
value per 0x01c100e7 100300 1
label "check file system"

keyValue
value String per 0x01c100e7 100301
label "status"
poll 1000

button
value per 0x01c100e7 100302 1
label "start tracelogger"

button
value per 0x01c100e7 100303 1
label "deactivate health"

BIOSCtrl
value int per 0x01c100e7 100305
label "core file device"
entry "eMMC" 0
entry "SD-Card" 1
entry "TMPFS" 2
entry "debug" 3
poll 1000

#######################
screen Persistence TechniSat
#######################
button
value per 0x01c100e7 100150 1
label "delete eMMC persistence"

button
value per 0x01c100e7 100151 1
label "delete EEPROM user data"

button
value per 0x01c100e7 100155 1
label "dump EEPROM data"
button

value per 0x01c100e7 100152 1
label "copy configuration to SDCard"

button
value per 0x01c100e7 100153 1
label "restore configuration from SDCard"

keyValue
value String per 0x01c100e7 100154
label "configuration transfer status"
poll 10000


#######################
screen Navigation_Dest-Input TechniSat
#######################

choice
value  per 0x01c500e6 100400
label "Development Area Search (on/off) :"

slider
value per 0x01c500e6 100401
