<?xml version="1.0" ?>
<MirrorlinkCommonConfig>
   <CommonConfig>
            <ClientID Value="VW-Mibstd2" />
            <SendClientFramebufferConfiguration Value="True" />
            <PreferredVncEncoding Value="TRLE" /> <!-- Raw, TRLE, ZRLE, JPEG, SLRLE, JRLE  -->
            <ClientSupportsTouchEvents Value="True" />   <!-- S3 Bug when True-->
            <ClientSupportsPointerEvents Value="True" />
            <ClientSupportsRTPClient Value="True" /> <!-- Disables clientside RTP-Server -->
            <ClientSupportsVoiceInput Value="True" /> <!-- Disables clientside RTP-Server -->
            <ClientSupportsVoiceControl Value="True" /> <!-- Disables clientside VoiceControl -->
            <ClientSupportsVoiceTelephony Value="False" /> <!-- Disables clientside Telephony -->
            <UseRGB565Format Value="True" />
            <ClientProfileSupportedRtpPayloadTypes Value="98,99" />
            <ClientProfileSupportedRtpAudioIpl Value="4800" />
            <ClientProfileSupportedRtpAudioMpl Value="9600" />
            <RtpGlobalTimeoutMultiplier Multiplier="1.3" /> <!-- double beginning from 0.1 -->
            <AudiosourceReleaseTimeout Milliseconds="3000" /> <!-- Timeout for releasing a Audiosource-->
            <PressureMask Value="1" />
            <DiscovererInterfacesToIgnore> <!-- Interfaces that should be ignored by the mirrorlink discoverer.  -->
               <Interface Value="fec0" />
               <Interface Value="lo0" />
               <Interface Value="pan0" />
               <Interface Value="virbr0" />
               <Interface Value="pflog0" />
               <Interface Value="tsd_imx60" />
               <Interface Value="ppp0" />
               <Interface Value="ax0" />
            </DiscovererInterfacesToIgnore>
            <ResetVNCAfterLaunchedApp Value="False" />   <!-- Resetting VNC Connection after a LaunchApplication, when the same URL is recevied -->
   </CommonConfig>
      <GEMDefaultConfig>
            <GEMIgnoreRestrictedMode Value="True" />
            <GEMIgnoreFramebufferBlocking Value="True" />
            <GEMIgnoreAudioBlocking Value="True" />
            <GEMDisableContentAttestationRequests Value="True" />
      </GEMDefaultConfig>
   <TesterConfig>
      <PrintFPS Value="False" />
      <VncReconnectDelay Milliseconds="1500" />
      <CTSTestModeEnabled Value="False" />   <!-- no connect to automatic problem after few disconnects -->
      <InterfaceScanOnStartup Value="False" />   <!-- scans all interfaces and enables the discoverer after the Startup/Swap -->
      <SetAllAppsDrivingCertifiedForTestingPurpose Value="True" />
      <TestingGetAppCertificate Value="False" /> <!-- only works, if SetAllAppsDrivingCertifiedForTestingPurpose = True -->
      <IgnoreEmptyDriverDistractionValues Value="True" /> <!--Ignoring Emtpy values from app for RestrictedAccesMode (TrustLevel,.)-->
      <RealVNCdiscovererSeverity Value="0" /> <!-- sets the logging severity for the discoverer-->
      <RealVNCViewerSeverity Value="0" /> <!-- sets the logging severity for the discoverer-->
      <RealVNCAudioSeverity Value="0" /> <!-- sets the logging severity for the discoverer-->
      <RealVNCEnablePerformanceMeasuring Value="" /> <!-- Possible Values are: FramesRendered:PixelThroughput:CallbackCompletion -->
   </TesterConfig>
   <DriverDistraction>
          <BlockFullFramebufferInNotification Value="False" />
          <!-- See Mirrorlink Spec Appendix A â€“ Application Context Information TrustLevel -->
          <AppAllowedTrustLevel Value="0x0000" />  <!-- Used in Discoverer (Applist) "%s:appInfo:trustLevel" -->
          <AppCategoryAllowedTrustLevel Value="0x0000" />  <!-- Used in ContextInformation  -->
          <ContentCategoryAllowedTrustLevel Value="0x0000" /> <!-- Used in ContextInformation -->
          <DisplayInfoAllowedMinTrustLevel Value="0x0000" /> <!-- Used in Discoverer (Applist) "%s:displayInfo:trustLevel" -->
          <AudioInfoAllowedMinTrustLevel Value="0x0000" /> <!-- Used in Discoverer (Applist) "%s:audioInfo:trustLevel" -->
          <AllowedAudioTypes>
                    <AudioType Value="application" />
                    <AudioType Value="phone" />
                    <AudioType Value="all" />
                    <AudioType Value="none" />
         </AllowedAudioTypes>
          <!-- Block individual VisualContentCategories -->
          <NotAllowedVisualContentCategories>
             <!-- <VisualContentCategory Value="0x00000000" /> -->
         </NotAllowedVisualContentCategories>
          <!-- Block individual AudioContentCategories -->
          <NotAllowedAudioContentCategories>
             <!-- <AudioContentCategory Value="0x00000000" /> -->
         </NotAllowedAudioContentCategories>
         <!-- Block individual ApplicationCategories -->
         <NotAllowedApplicationCategories>
         	<!-- <ApplicationCategory Value="0x00010000" /> -->
         </NotAllowedApplicationCategories>
         <SkipApplicationCategories> <!-- Used in Discoverer to Skip Applications in Applist  -->
            <SkippedApplicationCategory Value="0xF0000000" /> <!-- NoUI  -->
            <SkippedApplicationCategory Value="0xF0000001" /> <!-- NoUIAudioServer   -->
            <SkippedApplicationCategory Value="0xF0000002" /> <!-- NoUIAudioClient  -->
            <SkippedApplicationCategory Value="0xF0000010" /> <!-- NoUIVoiceCommandEngine -->
            <SkippedApplicationCategory Value="0xF0000020" /> <!-- NoUIConversationalAudio -->
            <SkippedApplicationCategory Value="0xF000FFFF" /> <!-- SwitchToClientNativeUI -->
         </SkipApplicationCategories>
         <AllowedPhoneAccesViewCategories> <!-- Used in Discoverer to find Applications for PhoneAccess -->
            <AllowedPhoneAccesViewCategory Value="0xF0000001" /> <!-- ServerFunctionality -->
            <AllowedPhoneAccesViewCategory Value="0x00010001" /> <!-- UIHomeScreen -->
            <AllowedPhoneAccesViewCategory Value="0x00010000" /> <!-- UI  -->
            <AllowedPhoneAccesViewCategory Value="0xFFFF0000" /> <!-- System -->
         </AllowedPhoneAccesViewCategories>
         <MandatoryCertVendorSpecificValues>
            <CertVendorSpecificValue Value="TOUCH" />
            <CertVendorSpecificValue Value="SPEECH" />
         </MandatoryCertVendorSpecificValues>
       </DriverDistraction>
       <SpellerConfig> <!-- ClientEventConfiguration -->
          <KnobKeysEnabled Value="False" />
          <DeviceKeysEnabled Value="False" />
          <MultimediaKeysEnabled Value="True" />
          <ITUSpellerEnabled Value="False" />
          <VirtualKeyboardTriggerEnabled Value="False" />
          <KeyEventListingEnabled Value="False" />
          <EventmappingEnabled Value="False" />
          <TimeoutKeyEventResponse Milliseconds="1000" /> <!-- Timeout triggered when Key event is sended an no Key Event List is responded -->
          <TimeoutKeyListingResponse Milliseconds="6000" /> <!-- Timeout triggered when a KeylistingRequest is sent and no defaultlist is received -->
       </SpellerConfig>
</MirrorlinkCommonConfig>
